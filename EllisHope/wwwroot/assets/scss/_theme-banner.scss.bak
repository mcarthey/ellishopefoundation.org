    @media #{$max-laptop} {
        padding-top: 200px;

        .main-title {
            font-size: 180px;
        }

        .description {
            font-size: 22px;
            line-height: 38px;
        }
    }

    @media #{$max-lg} {

        padding-top: 150px;

        .main-title {
            font-size: 120px;
        }

        .description {
            font-size: 22px;
        }
    }

    @media #{$max-md} {
        padding-bottom: 100px;

        .main-title {
            font-size: 120px;
        }

        .description {
            font-size: 18px;
        }

        .hero-video {
            display: none;
        }

        .social-links {
            right: 0;
        }
    }

    @media #{$max-xs} {
        .sub-title {
            font-size: 70px;
        }

        .main-title {
            font-size: 75px;
            margin-bottom: 40px;
        }

        .description {
            font-size: 20px;
            line-height: 34px;
            max-width: 100%;
        }

        .social-links {
            display: none;
        }
    }
}


