        background: $theme-color2;

        &::before {
            background: $white;
        }

    }

    .bg-shape {
        position: absolute;
        right: 0;
        top: 0;
        z-index: -1;
    }

    .popup-video {
        color: $theme-color2;
        font-size: 65px;
        background: rgba(255, 255, 255, 0.1);
        width: 150px;
